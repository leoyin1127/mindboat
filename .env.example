# Supabase Configuration
# Copy this file to .env and fill in your Supabase project details
# You can find these values in your Supabase project settings

VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Example:
# VITE_SUPABASE_URL=https://your-project-id.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Gemini API Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# ElevenLabs API Configuration
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
VITE_ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# Advanced Distraction Detection Configuration
VITE_SCREENSHOT_INTERVAL_MS=60000
VITE_CAMERA_CHECK_INTERVAL_MS=30000
VITE_DISTRACTION_THRESHOLD_MS=300000

# Spline 3D Scene
VITE_SPLINE_SCENE_URL=https://prod.spline.design/your-scene-id/scene.splinecode

# Voice Services (if using ElevenLabs for voice synthesis)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Feature Flags for gradual rollout
VITE_ENABLE_SPLINE_3D=true
VITE_ENABLE_VOICE_FEATURES=true
VITE_ENABLE_DISTRACTION_DETECTION=true

# Development flags
VITE_SHOW_DEV_TOOLS=true
VITE_ENABLE_PERFORMANCE_MONITORING=false